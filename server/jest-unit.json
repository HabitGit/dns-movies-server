{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".unit-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@shared/interfaces": "<rootDir>/libs/shared/src",
    "^@shared/dto": "<rootDir>/libs/shared/src/dto",
    "^@shared": "<rootDir>/libs/shared/src"
  }
}